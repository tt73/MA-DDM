#!/bin/bash -l
#SBATCH -J j2
#SBATCH -p public
#SBATCH -o out2
#SBATCH -p dms-cpu
#SBATCH --mail-user tt73@njit.edu
#SBATCH -A tt73
#SBATCH -t 8:0:0
#SBATCH --mem=16G
#SBATCH --nodes 2
#SBATCH --ntasks 2
module load gnu8 mpich petsc
N=300
mpirun ../../maddm -N $N -problem ex1 -sub_snes_max_it 1 -sub_ksp_rtol 1e-1 -sub_snes_linesearch_type l2 -snes_max_it 500
mpirun ../../maddm -N $N -problem ex2 -sub_snes_max_it 1 -sub_ksp_rtol 1e-1 -sub_snes_linesearch_type l2 -snes_max_it 500
mpirun ../../maddm -N $N -problem ex3 -sub_snes_max_it 1 -sub_ksp_rtol 1e-1 -sub_snes_linesearch_type l2 -snes_max_it 100
