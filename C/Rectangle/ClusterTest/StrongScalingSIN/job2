#!/bin/bash -l
#SBATCH -J j2
#SBATCH -p public
#SBATCH -o slurmout2
#SBATCH -p dms-cpu
#SBATCH --mail-user tt73@njit.edu
#SBATCH -A tt73
#SBATCH -t 8:0:0
#SBATCH --mem=16G
#SBATCH --nodes 2
#SBATCH --ntasks 2
#SBATCH --ntasks-per-node 1
#SBATCH --exclusive
module load gnu8 mpich petsc/3.12.0
N=300
mpirun ../../maddm -N $N -problem ex1 -sin -op 0.10 >> out2
mpirun ../../maddm -N $N -problem ex2 -sin -op 0.10 >> out2
mpirun ../../maddm -N $N -problem ex3 -sin -op 0.10 >> out2
mpirun ../../maddm -N $N -problem ex4 -sin -op 0.10 >> out2
